{
  "name": "saas-template.client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:prod": "tsc --noEmit && vite build --mode production",
    "build:analyze": "pnpm run build && pnpm run analyze",
    "analyze": "pnpx vite-bundle-analyzer dist/stats.html",
    "preview": "vite preview",
    "type-check": "tsc --noEmit --project tsconfig.app.json",
    "type-check:file": "tsc --noEmit --project tsconfig.single.json",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "quality-check": "pnpm run type-check && pnpm run lint && pnpm run test:run",
    "quality-check:full": "pnpm run type-check && pnpm run lint && pnpm run test:coverage && pnpm run build",
    "perf:audit": "lighthouse http://localhost:4173 --output=json --output=html --output-path=./performance-audit",
    "perf:build-size": "pnpm run build && du -sh dist/",
    "perf:dev": "pnpm run dev -- --host",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "build:analyze-full": "pnpm run build && pnpm run analyze:bundle && pnpm run analyze",
    "verify:production": "node scripts/verify-production-build.js",
    "build:production": "pnpm run build && pnpm run verify:production",
    "clean:use-client": "node scripts/clean-use-client.js",
    "build:clean": "pnpm run clean:use-client && pnpm run build:prod"
  },
